    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "\n",
    "# slice of the pie chart\n",
    "explode = (0, 0.1, 0, 0)  # only \"explode\" the 2nd slice\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.pie(sizes, explode=explode, labels=labels, autopct='%1.1f%%', startangle=140, colors=['gold', 'yellowgreen', 'lightcoral', 'lightskyblue'])\n",
    "plt.title('Exploded Pie Chart')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
