    "# Plot multiple lines on the same graph with markers\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y1, marker='o', linestyle='-', color='b', label='Line 1')\n",
    "plt.plot(x, y2, marker='s', linestyle='--', color='r', label='Line 2')\n",
    "plt.xlabel('X-axis')\n",
    "plt.ylabel('Y-axis')\n",
    "plt.title('Multiple Lines Plot with Markers')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
