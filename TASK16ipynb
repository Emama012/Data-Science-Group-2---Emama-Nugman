   "source": [
    "# basic pie chart\n",
    "sizes = [20, 30, 25, 25]\n",
    "labels = ['A', 'B', 'C', 'D']\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.pie(sizes, labels=labels, autopct='%1.1f%%', startangle=140, colors=['gold', 'yellowgreen', 'lightcoral', 'lightskyblue'])\n",
    "plt.title('Basic Pie Chart')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
